{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "react-app",
        "airbnb",
        "react-app/jest",
        "plugin:prettier/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "quotes":[
            "error",
            "double",
            {
                "avoidEscape": true
            }
        ],
        "react/prop-types": "off",
        "comma-dangle": "off",
        "react/react-in-jsx-scope": "off",
        "react/jsx-uses-react": "off",
        "react/jsx-props-no-spreading": "off",
        "no-shadow": "off"
    }
}
